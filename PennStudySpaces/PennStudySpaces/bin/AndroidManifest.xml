<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.upenn.cis573"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="9" />

    <application
        android:icon="@drawable/iconapp" android:antialias="true">
        <activity android:screenOrientation="portrait"
            android:name=".SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:screenOrientation="portrait" android:name=".SearchActivity" android:label="@string/app_name"></activity>
        <activity android:screenOrientation="portrait" android:name=".StudySpaceListActivity" android:label="@string/app_name"></activity>
        <activity android:screenOrientation="portrait" android:name=".StudySpaceDetails" android:label="@string/app_name"></activity>
        <activity android:screenOrientation="portrait" android:name=".Meme" android:label="@string/app_name"></activity>
        <activity android:screenOrientation="portrait" android:name=".About" android:label="@string/app_name"></activity>
        <activity android:screenOrientation="portrait" android:name=".Help" android:label="@string/app_name"></activity>
        <activity android:screenOrientation="portrait" android:name=".CustomMap" android:label="@string/app_name"></activity>
        <uses-library android:name="com.google.android.maps" />
    </application>
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name = "android.permission.ACCESS_COARSE_LOCATION" />
	
</manifest>